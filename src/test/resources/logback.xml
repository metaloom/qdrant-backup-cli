<configuration>
	<appender name="STDOUT"
		class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger - %msg%n
			</pattern>
		</encoder>
	</appender>

	<logger name="io.metaloom" level="WARN" />

	<root level="error">
		<appender-ref ref="STDOUT" />
	</root>

</configuration>